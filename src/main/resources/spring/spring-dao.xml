    </bean>

    <bean id="dynamicDao" class="com.rcswu.dao.impl.DynamicDaoImpl">
        <property name="hibernateTemplate" ref="hibernateTemplate"></property>
    </bean>

    <bean id="letterDao" class="com.rcswu.dao.impl.LetterDaoImpl">
        <property name="hibernateTemplate" ref="hibernateTemplate"></property>
    </bean>
